{
  "name": "mce",
  "type": "module",
  "version": "0.16.3",
  "description": "A headless infinite canvas editor framework built on WebGL rendering, supports exporting to image, video, and PPT. Only the ESM.",
  "author": "wxm",
  "license": "MIT",
  "homepage": "https://github.com/qq15725/mce",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/mce.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/mce/issues"
  },
  "keywords": [
    "vue",
    "webgl",
    "modern",
    "infinite",
    "canvas",
    "editor",
    "mce",
    "typescript"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles": {
      "default": "./dist/index.css"
    },
    "./*.mjs": "./*.js",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "styles": "dist/index.css",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:code": "vite build",
    "build:tsc": "NODE_ENV=production vue-tsc --emitDeclarationOnly --project tsconfig.json",
    "build:preset": "jiti ./scripts/preset.ts",
    "build:dts": "jiti ./scripts/dts.ts",
    "build:doc": "typedoc",
    "build": "pnpm build:code && pnpm build:tsc && pnpm build:dts",
    "lint": "eslint src",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.10",
    "@vueuse/components": "^14.2.0",
    "@vueuse/core": "^14.2.0",
    "diff": "^8.0.3",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.23",
    "modern-canvas": "^0.15.3",
    "modern-font": "^0.4.4",
    "modern-idoc": "^0.10.21",
    "modern-text": "^1.10.15",
    "yjs": "^13.6.29"
  },
  "peerDependencies": {
    "lottie-web": "^5",
    "modern-gif": "^2",
    "typescript": ">=4.7",
    "vue": "^3.5.0",
    "yoga-layout": "^3"
  },
  "peerDependenciesMeta": {
    "lottie-web": {
      "optional": true
    },
    "modern-gif": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "yoga-layout": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^6.0.4",
    "jiti": "^2.6.1",
    "modern-gif": "^2.0.4",
    "sass-embedded": "^1.97.3",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0"
  }
}
